{
  "name": "kct-ai-chat",
  "version": "1.0.0",
  "description": "KCT Menswear AI Chat Service - Conversational AI for fashion consultation",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "train": "node run-training.js",
    "train:local": "CHAT_API_URL=http://localhost:3002 node run-training.js"
  },
  "keywords": [
    "ai",
    "chat",
    "menswear",
    "fashion",
    "openai",
    "conversational-ai"
  ],
  "author": "KCT Menswear",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "zod": "^3.22.4",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "@supabase/supabase-js": "^2.39.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
